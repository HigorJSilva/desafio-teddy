{
  "name": "desafio-teddy",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npx nodemon -L",
    "dev:debug": "nodemon --inspect src/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir build --copy-files",
    "build:tsc": "rimraf ./build && tsc && resolve-tspaths",
    "start": "npm run build && node build/server.js",
    "start:debug": "npm start && node --inspect build/server.js",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint — fix",
      "prettier — write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.7",
    "babel-cli": "^6.26.0",
    "eslint": "^9.8.0",
    "husky": "^9.1.4",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "6.14.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mysql2": "^3.11.0",
    "reflect-metadata": "^0.2.2",
    "resolve-tspaths": "^0.8.19",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20"
  }
}
